#include <iostream>

using namespace std;

int main(){
    int x[1000];
    int n;
    cin >> n;
    for(int i = 0; i < n; i++){
        cin >> x[i];
    }
    int max = x[0];
    for(int i = 0; i < n; i++){
        if( x[i] > max){
            max = x[i];
        }
    }
    int min = x[0];
    for(int i = 0; i < n; i++){
        if(x[i] < min){
            min = x[i];
        }
    }
    cout << max + min;
    return 0;
}
